<Window x:Name="window" x:Class="OptionFileGenerator.EditCanvas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OptionFileGenerator"
        mc:Ignorable="d"
        Title="EditCanvas" Height="633" Width="800" SizeChanged="window_SizeChanged">

    <DockPanel x:Name="stackPan">
        <Canvas Margin="0"  x:Name="CanvasEdit" MouseDown="CanvasEdit_MouseDown" SizeChanged="CanvasEdit_SizeChanged" Loaded="CanvasEdit_Loaded"/>
    </DockPanel>

</Window>
